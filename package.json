{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "prisma generate && yarn graphql:generate && yarn abis:generate && next dev",
    "build": "prisma generate && yarn graphql:generate && yarn abis:generate && next build",
    "vercel-build": "yum install libuuid-devel libmount-devel zlib && cp /lib64/{libuuid,libmount,libblkid,libz}.so.1 node_modules/canvas/build/Release/ && npm run build",
    "start": "next start",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,json}\"",
    "prepare": "husky install",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test-coverage": "yarn test --collect-coverage=true",
    "graphql:generate": "yarn graphql:generate:nft-loans && yarn graphql:generate:nft-sales && yarn graphql:generate:eip721",
    "graphql:generate:eip721": "graphql-codegen --config codegen/eip721.yml",
    "graphql:generate:nft-loans": "graphql-codegen --config codegen/nftLoans.yml",
    "graphql:generate:nft-sales": "graphql-codegen --config codegen/nftSales.yml",
    "abis:generate": "yarn typechain --target ethers-v5 --out-dir ./types/generated/abis './abis/*.json'",
    "prisma-migrate:local": "dotenv -e .env.local -- npx prisma migrate dev",
    "prisma-migrate:prod": "dotenv -e .env.production.local -- npx prisma migrate deploy",
    "prisma:studio": "dotenv -e .env.test npx prisma studio"
  },
  "dependencies": {
    "@ethersproject/providers": "5.6.1",
    "@hookform/resolvers": "2.8.8",
    "@pinata/ipfs-gateway-tools": "1.1.6",
    "@prisma/client": "3.11.0",
    "@rainbow-me/rainbowkit": "0.0.2",
    "@sentry/nextjs": "6.19.6",
    "@web3-react/core": "6.1.9",
    "@web3-react/injected-connector": "6.0.7",
    "@web3-react/walletconnect-connector": "6.2.13",
    "@web3-react/walletlink-connector": "6.2.13",
    "@xstate/react": "2.0.1",
    "aws-sdk": "2.1098.0",
    "dayjs": "1.11.0",
    "discord.js": "12.5.3",
    "ethers": "5.6.2",
    "graphql": "16.3.0",
    "lodash": "4.17.21",
    "mjml": "4.12.0",
    "next": "12.1.0",
    "normalize.css": "8.0.1",
    "prisma": "3.11.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.28.1",
    "react-select": "5.2.2",
    "reakit": "1.3.11",
    "sharp": "0.30.3",
    "swr": "1.2.2",
    "twitter-api-client": "1.5.2",
    "urql": "2.2.0",
    "wagmi": "0.2.24",
    "xstate": "4.30.6",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@babel/core": "7.17.8",
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/typed-document-node": "2.2.8",
    "@graphql-codegen/typescript": "2.4.8",
    "@graphql-codegen/typescript-operations": "2.3.5",
    "@graphql-codegen/typescript-urql": "3.5.6",
    "@graphql-codegen/urql-introspection": "2.1.1",
    "@graphql-typed-document-node/core": "3.1.1",
    "@next/bundle-analyzer": "12.1.0",
    "@storybook/addon-actions": "6.4.19",
    "@storybook/addon-essentials": "6.4.19",
    "@storybook/addon-links": "6.4.19",
    "@storybook/react": "6.4.19",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.4",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/user-event": "13.5.0",
    "@typechain/ethers-v5": "9.0.0",
    "@types/lodash": "4.14.180",
    "@types/mjml": "4.7.0",
    "@types/node": "17.0.22",
    "@types/react": "17.0.42",
    "@types/sharp": "0.30.0",
    "babel-jest": "27.5.1",
    "dotenv-cli": "5.0.0",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "husky": "7",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.5.1",
    "jest-fetch-mock": "3.0.3",
    "lint-staged": "12.3.7",
    "node-mocks-http": "1.11.0",
    "prettier": "2.6.0",
    "react-test-renderer": "17.0.2",
    "storybook-addon-next-router": "3.1.1",
    "storybook-css-modules-preset": "1.1.1",
    "ts-node": "10.7.0",
    "typechain": "7.0.1",
    "typescript": "4.6.2",
    "yarn": "1.22.18"
  },
  "resolutions": {
    "canvas": "2.6.1"
  },
  "lint-staged": {
    "*.{ts,tsx,js,json}": [
      "prettier --write"
    ]
  }
}

{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn typechain --target ethers-v5 --out-dir ./abis/types './abis/*.json' && next dev",
    "build": "yarn typechain --target ethers-v5 --out-dir ./abis/types './abis/*.json' && next build",
    "start": "next start",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "yarn eslint --no-error-on-unmatched-pattern $(git diff --name-only --cached | grep -E *.tsx?$ | xargs)",
    "lint:all": "yarn eslint .",
    "lint:in": "yarn eslint",
    "preinstall": "git config core.hooksPath git-hooks",
    "format": "yarn prettier --write \"**/*.{ts,tsx,js,json}\""
  },
  "dependencies": {
    "@ethersproject/keccak256": "^5.4.0",
    "@nfte/handler": "^1.0.5",
    "@nfte/react": "^1.0.4",
    "cors": "^2.8.5",
    "ethers": "^5.0.26",
    "is-ipfs": "^6.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "next": "10.0.5",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@storybook/addon-actions": "^6.3.10",
    "@storybook/addon-essentials": "^6.3.10",
    "@storybook/addon-links": "^6.3.10",
    "@storybook/react": "^6.3.10",
    "@typechain/ethers-v5": "^7.2.0",
    "@types/node": "^14.14.21",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-loader": "^8.2.2",
    "eslint": "^8.0.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-plugin-import": "^2.25.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "prettier": "^2.4.1",
    "typechain": "^5.2.0",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.1"
  }
}

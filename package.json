{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn graphql:generate && yarn abis:generate && next dev",
    "build": "yarn graphql:generate && yarn abis:generate && next build",
    "start": "next start",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,json}\"",
    "prepare": "husky install",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test-coverage": "yarn test --collect-coverage=true",
    "graphql:generate": "yarn graphql:generate:nft-loans && yarn graphql:generate:nft-sales",
    "graphql:generate:nft-loans": "graphql-codegen --config codegen/nftLoans.yml",
    "graphql:generate:nft-sales": "graphql-codegen --config codegen/nftSales.yml",
    "abis:generate": "yarn typechain --target ethers-v5 --out-dir ./types/generated/abis './abis/*.json'",
    "prisma-migrate:test": "dotenv -e .env.test -- npx prisma migrate dev --name init-test",
    "prisma:studio": "dotenv -e .env.test npx prisma studio",
    "notifications:cron": "npx ts-node --skip-project lib/notifications/dailyCron.ts"
  },
  "dependencies": {
    "@ethersproject/providers": "^5.5.1",
    "@hookform/resolvers": "^2.8.8",
    "@pinata/ipfs-gateway-tools": "^1.1.6",
    "@prisma/client": "^3.9.1",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/walletconnect-connector": "^6.2.8",
    "@web3-react/walletlink-connector": "^6.2.8",
    "@xstate/react": "^1.6.3",
    "dayjs": "^1.10.7",
    "ethers": "^5.4.6",
    "graphql": "^16.0.1",
    "lodash": "^4.17.21",
    "next": "^12.0.9",
    "normalize.css": "^8.0.1",
    "prisma": "^3.9.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-hook-form": "^7.25.0",
    "react-select": "^5.2.1",
    "reakit": "^1.3.10",
    "swr": "^1.1.2",
    "urql": "^2.0.5",
    "xstate": "^4.28.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@graphql-codegen/cli": "2.3.1",
    "@graphql-codegen/typescript": "^2.4.2",
    "@graphql-codegen/typescript-operations": "^2.2.2",
    "@graphql-codegen/typescript-urql": "^3.4.2",
    "@graphql-codegen/urql-introspection": "2.1.1",
    "@next/bundle-analyzer": "^12.0.2",
    "@storybook/addon-actions": "^6.3.10",
    "@storybook/addon-essentials": "^6.3.10",
    "@storybook/addon-links": "^6.3.10",
    "@storybook/react": "^6.3.10",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@typechain/ethers-v5": "^7.2.0",
    "@types/lodash": "^4.14.176",
    "@types/node": "^17.0.16",
    "@types/react": "^17.0.0",
    "babel-jest": "^27.3.1",
    "dotenv-cli": "^5.0.0",
    "eslint": "<8.0.0",
    "eslint-config-next": "12.0.2",
    "husky": ">=6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": ">=10",
    "node-mocks-http": "^1.11.0",
    "prettier": "^2.4.1",
    "react-test-renderer": "^17.0.2",
    "storybook-addon-next-router": "^3.1.0",
    "storybook-css-modules-preset": "^1.1.1",
    "ts-node": "^10.5.0",
    "typechain": "^5.0.0",
    "typescript": "^4.5.5",
    "yarn": "^1.22.17"
  },
  "lint-staged": {
    "*.{ts,tsx,js,json}": [
      "prettier --write"
    ]
  }
}
